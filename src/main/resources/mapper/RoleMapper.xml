<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="app.lifeni.bms.dao.RoleDao">

    <select id="queryAllRoles" resultType="app.lifeni.bms.entity.model.Role">
        select *
        from `role`
    </select>

    <select id="queryRoleById" resultType="app.lifeni.bms.entity.model.Role">
        select *
        from `role`
        where roleId = #{roleId}
    </select>

</mapper>
